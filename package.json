{
  "name": "L107",
  "version": "0.0.0",
  "description": "Compiler as a service.",
  "author": "Art Compiler LLC",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "build-dev": "make -C mathcore; make -C translatex; node ./tools/build --dev",
    "build": "node ./tools/build",
    "smoke": "node tools/test.js --smoke",
    "test": "node tools/test.js",
    "test-bugs": "mocha --reporter spec --recursive --delay --bugs",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "decimal.js": "^10.2.0",
    "domready": "^1.0.8",
    "express": "^4.0.x",
    "hashids": "1.1.1",
    "mathjax-node": "^1.1.0",
    "morgan": "^1.9.1",
    "request": "^2.88.0"
  },
  "engines": {
    "node": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artcompiler/L107"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel": "^6.0.x",
    "babel-cli": "^6.4.5",
    "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babelify": "^7.0.2",
    "browserify": "^12.0.1",
    "chai": "^4.2.0",
    "d3": "4.7.1",
    "flow-bin": "^0.32.0",
    "json-diff": "^0.5.4",
    "mocha": "^6.1.3",
    "react": "0.14.5",
    "react-dom": "0.14.5",
    "supertest": "^4.0.2",
    "uglify-js": "^2.7.3"
  }
}
